//add brackets to the whole denominator, otherwise error
//create a function; calculate the sum of 1/(i*(i+1))*(-1)^(i+1),i=1,...n

n=int(input())
def sum(n):
    sum=0
    for i in range(1,n+1):
        print(i)
        if i&1:
            sum+=1/(i*(i+1))
            print('odd')
            print(sum)
        else :
            sum-=1/(i*(i+1))
            print('even')
            print(sum)
    print(sum)
sum(n)
